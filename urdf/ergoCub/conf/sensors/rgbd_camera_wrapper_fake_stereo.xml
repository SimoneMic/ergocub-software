
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE robot PUBLIC "-//YARP//DTD yarprobotinterface 3.0//EN" "http://www.yarp.it/DTD/yarprobotinterfaceV3.0.dtd">

<devices>
<device xmlns:xi="http://www.w3.org/2001/XInclude" name="depthWrapYLeft" type="rgbdSensor_nws_yarp">
    <param name="period">0.033</param>
    <param name="name"> ${portprefix}/depthCameraLeft </param>
    <action phase="startup" level="5" type="attach">
        <paramlist name="networks">
            <elem name="the_device"> rgbdCameraLeft </elem>
        </paramlist>
    </action>
    <action phase="shutdown" level="5" type="detach" />
</device>

<device xmlns:xi="http://www.w3.org/2001/XInclude" name="depthWrapYRight" type="rgbdSensor_nws_yarp">
    <param name="period">0.033</param>
    <param name="name"> ${portprefix}/depthCameraRight </param>
    <action phase="startup" level="5" type="attach">
        <paramlist name="networks">
            <elem name="the_device"> rgbdCameraRight </elem>
        </paramlist>
    </action>
    <action phase="shutdown" level="5" type="detach" />
</device>

</devices>
